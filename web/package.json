{
  "name": "play-by-play-web",
  "private": true,
  "version": "0.0.1",
  "description": "Baskeyball simulation",
  "main": "",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "dist": "npm run copy & webpack --env=dist",
    "dist:dev": "npm run copy & webpack --env=dev",
    "dist:test": "npm run copy & webpack --env=test",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "serve:test": "node server.js --env=test",
    "start": "node server.js --env=dev"
  },
  "repository": "",
  "keywords": [],
  "author": "Scott Willson",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "copyfiles": "^2.1.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "glob": "^7.1.3",
    "minimist": "^1.2.0",
    "null-loader": "^0.1.1",
    "react-hot-loader": "^4.3.11",
    "rimraf": "^2.4.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "core-js": "^2.5.7",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "superagent": "^4.0.0-beta.5"
  }
}
