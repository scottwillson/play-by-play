{
  "name": "play-by-play-web",
  "private": true,
  "version": "0.0.1",
  "description": "Baskeyball simulation",
  "main": "",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "dist": "npm run copy & webpack --env=dist",
    "dist:dev": "npm run copy & webpack --env=dev",
    "dist:test": "npm run copy & webpack --env=test",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "serve:test": "node server.js --env=test",
    "start": "node server.js --env=dev"
  },
  "repository": "",
  "keywords": [],
  "author": "Scott Willson",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "copyfiles": "^1.2",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^1.1.11",
    "glob": "^7.1.3",
    "minimist": "^1.2.0",
    "npm-install-webpack-plugin": "github:webpack-contrib/npm-install-webpack-plugin",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "react-hot-loader": "^3.1",
    "rimraf": "^2.4.3",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "core-js": "^2.5.7",
    "normalize.css": "^7.0.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "superagent": "^3.8.3"
  }
}
